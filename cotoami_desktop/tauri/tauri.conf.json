{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "cd ui && yarn && yarn run build",
    "frontendDist": "../ui/dist",
    "devUrl": "http://localhost:5173/"
  },
  "bundle": {
    "active": true,
    "category": "DeveloperTool",
    "copyright": "",
    "targets": "all",
    "externalBin": [],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": "",
      "wix": {
        "language": [
          "en-US",
          "ja-JP"
        ]
      }
    },
    "longDescription": "",
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": "-"
    },
    "resources": [
      "geomap/*"
    ],
    "shortDescription": "",
    "linux": {
      "deb": {
        "depends": []
      }
    }
  },
  "productName": "Cotoami",
  "mainBinaryName": "Cotoami",
  "identifier": "me.cotoa.dev",
  "plugins": {},
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Cotoami",
        "width": 900,
        "height": 800,
        "hiddenTitle": true,
        "titleBarStyle": "Overlay",
        "fileDropEnabled": false,
        "useHttpsScheme": true
      }
    ],
    "security": {
      "assetProtocol": {
        "scope": [
          "**/*"
        ],
        "enable": true
      },
      "csp": null
    }
  }
}